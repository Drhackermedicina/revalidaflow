{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npm",
      "args": [
        "run",
        "mcp:serve"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    }
  }
}