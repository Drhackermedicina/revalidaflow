# Configurações do Firebase
FIREBASE_PROJECT_ID=revalida-companion
FIREBASE_CLIENT_EMAIL=your-service-account@revalida-companion.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
FIREBASE_DATABASE_URL=https://revalida-companion-default-rtdb.firebaseio.com

# Configurações do Sentry
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_TRACES_SAMPLE_RATE=0.1

# Configurações da API Gemini
GEMINI_API_KEY=your-gemini-api-key

# Configurações do Google Cloud Speech
GOOGLE_APPLICATION_CREDENTIALS=./config/service-account.json

# Configurações do servidor
PORT=3000
NODE_ENV=production

# Configurações de CORS
CORS_ORIGIN=https://revalida-companion.web.app